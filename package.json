{
  "name": "vsix-info",
  "description": "Get publisher name and version of a vsix file.",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint -f unix .",
    "inspect": "jsinspect",
    "prettier": "prettier --list-different \"./**/**.js\"",
    "pretest": "npm run lint && npm run inspect && npm run prettier",
    "test": "nyc mocha",
    "test:watch": "nyc mocha --watch",
    "posttest": "nyc check-coverage --statements 90 --branches 90 --functions 90 --lines 90"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "author": "Julien Croain <julien.croain@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:JulienCroain/vsix-info.git"
  },
  "dependencies": {
    "jszip": "^3.1.5",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "jsinspect": "^0.12.7",
    "mocha": "^4.0.1",
    "nyc": "^13.1.0",
    "prettier": "^1.14.2"
  }
}
